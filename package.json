{
  "name": "@tedcryptoorg/cosmos-signer",
  "version": "0.0.1",
  "description": "Cosmos Signer - A library for signing transactions for Cosmos SDK chains",
  "scripts": {
    "test": "jest --passWithNoTests`",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "keywords": [],
  "author": "Josh_Lopes<josluis.lopes@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/TedcryptoOrg/cosmos-signer",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "dependencies": {
    "@cosmjs/amino": "^0.29.5",
    "@cosmjs/encoding": "^0.29.5",
    "@cosmjs/proto-signing": "^0.29.5",
    "@cosmjs/stargate": "^0.29.5",
    "@cosmjs/utils": "^0.29.5",
    "@tedcryptoorg/cosmos-directory": "^0.0.5",
    "axios": "^1.2.5",
    "axios-retry": "^3.4.0",
    "compare-versions": "^6.0.0-rc.1",
    "cosmjs-types": "^0.6.1",
    "lodash": "^4.17.21",
    "long": "^5.2.1",
    "mathjs": "^11.5.1",
    "moment": "^2.29.4",
    "parse-duration": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.6",
    "@types/lodash": "^4.14.191",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ]
}
